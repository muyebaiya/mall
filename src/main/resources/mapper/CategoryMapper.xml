<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csu.mall.dao.CategoryMapper">
    <select id="queryById" resultType="com.csu.mall.pojo.Category">
        select id,parent_id,name,status,sort_order,create_time,update_time from mall_category where id = #{id}
    </select>
</mapper>